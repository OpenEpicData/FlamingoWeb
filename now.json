{
  "name": "FlamingoWeb",
  "version": 2,
  "builds": [
    {
      "src": "nuxt.config.js",
      "use": "@nuxtjs/now-builder",
      "config": {
        "serverFiles": [
          "middleware/**",
          "plugins/**",
          "lib/**",
          "package.json",
          "vuetify.options.js"
        ]
      }
    }
  ],
  "routes": [
    { "src": "^/(.*)", "dest": "/" }
  ]
}